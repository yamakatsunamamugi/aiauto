    "logged_in_indicators": [
      "[data-testid*=\"user\"]",
      "[data-testid*=\"avatar\"]",
      "[aria-label*=\"user\"]",
      "[aria-label*=\"profile\"]",
      ".user-avatar",
      ".profile-button",
      "button[aria-haspopup=\"menu\"]"
