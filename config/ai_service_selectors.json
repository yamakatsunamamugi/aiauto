  "fallback_strategies": {
    "text_by_content": {
      "login_buttons": ["Sign in", "Log in", "Login"],
      "send_buttons": ["Send", "Submit", "Generate"],
      "model_buttons": ["Model", "GPT", "Claude", "Gemini"]
